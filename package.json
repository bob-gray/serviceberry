{
  "name": "serviceberry",
  "version": "0.1.3",
  "description": "Simple HTTP service framework",
  "main": "src/main.js",
  "files": [
    "src"
  ],
  "scripts": {
    "build": "npm run lint && npm run test-coverage",
    "lint": "eslint src/**",
    "lint:spec": "eslint spec/**",
    "test": "jasmine",
    "test-coverage": "nyc --include=src/**/*.js --all --reporter=lcov --reporter=text npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bob-gray/serviceberry.git"
  },
  "keywords": [
    "http",
    "services",
    "api",
    "rest"
  ],
  "author": "Bob Gray <gray.bob98@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bob-gray/serviceberry/issues"
  },
  "homepage": "https://github.com/bob-gray/serviceberry#readme",
  "dependencies": {
    "accepts": "^1.3.5",
    "content-type": "^1.0.4",
    "serviceberry-form": "^1.0.0",
    "serviceberry-json": "^1.0.1",
    "solv": "^1.0.1"
  },
  "devDependencies": {
    "eslint": "^4.18.2",
    "jasmine": "^3.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "node-mocks-http": "^1.5.8",
    "nyc": "^11.6.0"
  }
}
